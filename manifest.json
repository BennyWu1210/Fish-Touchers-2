{
    "name": "[temp] Fish Touchers II",
    "description": "[temp] the return of fish touching",
    "version": "0.0",
    "manifest_version": 3,

    "action": {
        "default_popup": "index.html",
        "default_icon": "touch.png"
    },
    "permissions": ["scripting","activeTab", "storage", "windows", "tabs"],
    "host_permissions": ["https://*/", "chrome-extension://*"],
    "web_accessible_resources": [
        {
          "resources": ["result_page_1.html", "result_page_2.html", "result_page_3.html"],
          "matches": ["https://*/*", "http://*/*"]
        }
      ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
   

  }